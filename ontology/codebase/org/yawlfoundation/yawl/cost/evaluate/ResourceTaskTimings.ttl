    yawl:returnType "long" ;
    yawl:signature "long getBusyTime(String instance)" ;
    rdfs:comment """@param instance the instance id of the work item @return the number of msecs that expired from when the work item was started until when it was completed or cancelled""" ;
    yawl:modifiers "public" ;
    .

