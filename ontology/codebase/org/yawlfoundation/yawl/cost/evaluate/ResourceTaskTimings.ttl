    rdfs:label "getAssignedTime" ;
    yawl:returnType "long" ;
    yawl:signature "long getAssignedTime()" ;
    rdfs:comment """@return the number of msecs that expired from when the task was offered until when it was allocated to a resource""" ;
    yawl:modifiers "public" ;
    .

# Method: ResourceTaskTimings.getAllocatedTime
yawl:ResourceTaskTimings_getAllocatedTime_3 a yawl:Method ;
    rdfs:label "getAllocatedTime" ;
    yawl:returnType "long" ;
    yawl:signature "long getAllocatedTime(String instance)" ;
    rdfs:comment """@param instance the instance id of the work item @return the number of msecs that expired from when the work item was allocated until when it was started by a resource""" ;
    yawl:modifiers "public" ;
    .

