# Method: YPredicateParser.parse
yawl:YPredicateParser_parse_1 a yawl:Method ;
    rdfs:label "parse" ;
    yawl:returnType "String" ;
    yawl:signature "String parse(String s)" ;
    rdfs:comment """Parses a string, replacing substrings within of the form ${expression} with the result of the expression evaluation @param s the string to parse @return s, with any embedded expressions replaced with their evaluations""" ;
    yawl:modifiers "public" ;
    .

# Method: YPredicateParser.valueOf
yawl:YPredicateParser_valueOf_2 a yawl:Method ;
    rdfs:label "valueOf" ;
    yawl:returnType "String" ;
    yawl:signature "String valueOf(String s)" ;
    rdfs:comment """Evaluates an expression and returns the result. Subclasses should override this class to do their own expression evaluations before calling this version for any general expression evaluations. @param s the string representing the expression, of the form ${expression} @return the result of the expression evaluation, or the unchanged string if the expression is unrecognised""" ;
    yawl:modifiers "protected" ;
    .

