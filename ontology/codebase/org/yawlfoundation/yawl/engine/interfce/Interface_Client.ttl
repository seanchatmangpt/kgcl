    rdfs:label "executePost" ;
    yawl:returnType "String" ;
    yawl:signature "String executePost(String urlStr, Map paramsMap)" ;
    rdfs:comment """Executes a HTTP POST request on the url specified. @param urlStr the URL to send the POST to @param paramsMap a set of attribute-value pairs that make up the posted data @return the result of the POST request @throws IOException when there's some kind of communication problem""" ;
    yawl:modifiers "protected" ;
    .

# Method: Interface_Client.executeGet
yawl:Interface_Client_executeGet_2 a yawl:Method ;
    rdfs:label "executeGet" ;
    yawl:returnType "String" ;
    yawl:signature "String executeGet(String urlStr, Map paramsMap)" ;
    rdfs:comment """Executes a rerouted HTTP GET request as a POST on the specified URL @param urlStr the URL to send the GET to @param paramsMap a set of attribute-value pairs that make up the posted data @return the result of the request @throws IOException when there's some kind of communication problem""" ;
    yawl:modifiers "protected" ;
    .

