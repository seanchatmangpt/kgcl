    yawl:returnType "Participant" ;
    yawl:signature "Participant performAllocation(Set resSet, WorkItemRecord wir)" ;
    rdfs:comment """selects the Participant with the least number of workitems in the 'allocated' workqueue, or the first participant with an empty queue  @param resSet the set of Participants to compare @param wir the work item to allocate @return the Participant with the shortest queue""" ;
