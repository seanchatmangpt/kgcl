# Class: org.yawlfoundation.yawl.stateless.elements.YAtomicTask
yawl:YAtomicTask a yawl:Class ;
    rdfs:label "YAtomicTask" ;
    yawl:inPackage yawl:org_yawlfoundation_yawl_stateless_elements ;
    yawl:filePath "vendors/yawl-v5.2/src/org/yawlfoundation/yawl/stateless/elements/YAtomicTask.java"^^xsd:string ;
    rdfs:comment """A YAtomicTask object is the executable equivalent of the Atomic Task in the YAWL language. They have the same properties and behaviour. @author Lachlan Aldred @author Michael Adams (v2.0 and later) @since 0.1""" ;
    yawl:extends yawl:YTask ;
    yawl:modifiers "public" ;
    yawl:hasMethod yawl:YAtomicTask_startOne_1 ;
    yawl:hasMethod yawl:YAtomicTask_isRunning_2 ;
    yawl:hasMethod yawl:YAtomicTask_cancel_3 ;
    yawl:hasMethod yawl:YAtomicTask_cancel_4 ;
    yawl:hasMethod yawl:YAtomicTask_cancelBusyWorkItem_5 ;
    yawl:hasMethod yawl:YAtomicTask_cancelWorkItem_6 ;
    yawl:hasMethod yawl:YAtomicTask_t_rollBackToFired_7 ;
    yawl:hasMethod yawl:YAtomicTask_clone_8 ;
    yawl:hasMethod yawl:YAtomicTask_verify_9 ;
    .


# Method: YAtomicTask.startOne
yawl:YAtomicTask_startOne_1 a yawl:Method ;
    rdfs:label "startOne" ;
    yawl:returnType "void" ;
    yawl:signature "void startOne(YNetRunner parentRunner, YIdentifier id)" ;
    rdfs:comment """Changes the inner state of an atomic task from entered to executing. @param parentRunner @param id the identifier to move from inner marking 'entered' to inner marking""" ;
    yawl:modifiers "protected" ;
    .

# Method: YAtomicTask.isRunning
yawl:YAtomicTask_isRunning_2 a yawl:Method ;
    rdfs:label "isRunning" ;
    yawl:returnType "boolean" ;
    yawl:signature "boolean isRunning()" ;
    rdfs:comment """Checks that a task is currently executing. @return true if the task has 'executing' state.""" ;
    yawl:modifiers "public" ;
    .

# Method: YAtomicTask.cancel
yawl:YAtomicTask_cancel_3 a yawl:Method ;
    rdfs:label "cancel" ;
    yawl:returnType "void" ;
    yawl:signature "void cancel()" ;
    rdfs:comment """Cancels a task.""" ;
    yawl:modifiers "synchronized" ;
    yawl:modifiers "public" ;
    .

# Method: YAtomicTask.cancel
yawl:YAtomicTask_cancel_4 a yawl:Method ;
    rdfs:label "cancel" ;
    yawl:returnType "void" ;
    yawl:signature "void cancel(YIdentifier caseID)" ;
    rdfs:comment """Cancels the task. If the task is not currently executing, first check that the workitem is still referenced by the engine before attempting the cancellation. @param caseID the case identifier of this atomic task.""" ;
    yawl:modifiers "synchronized" ;
    yawl:modifiers "public" ;
    .

