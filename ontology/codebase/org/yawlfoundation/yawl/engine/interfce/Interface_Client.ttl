    rdfs:label "initPostConnection" ;
    yawl:returnType "HttpURLConnection" ;
    yawl:signature "HttpURLConnection initPostConnection(String urlStr)" ;
    rdfs:comment """Initialises a HTTP POST connection @param urlStr the url to connect to @return an initialised POST connection @throws IOException when there's some kind of communication problem""" ;
    yawl:modifiers "protected" ;
    .

