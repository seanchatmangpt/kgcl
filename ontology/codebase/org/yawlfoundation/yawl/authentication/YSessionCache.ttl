yawl:YSessionCache_getSession_5 a yawl:Method ;
    rdfs:label "getSession" ;
    yawl:returnType "YSession" ;
    yawl:signature "YSession getSession(String handle)" ;
    rdfs:comment """Gets the session associated with a session handle. @param handle a session handle. @return the session object associated with the handle, or null if the handle is invalid or inactive.""" ;
    yawl:modifiers "public" ;
    .

# Method: YSessionCache.expire
yawl:YSessionCache_expire_6 a yawl:Method ;
    rdfs:label "expire" ;
    yawl:returnType "void" ;
    yawl:signature "void expire(String handle)" ;
    rdfs:comment """Removes a session from the set of active sessions after an idle timeout. Also writes the expiration to the session audit log. @param handle the session handle of the session to remove.""" ;
    yawl:modifiers "public" ;
    .

