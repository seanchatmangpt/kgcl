# Class: org.yawlfoundation.yawl.resourcing.allocators.AbstractAllocator
yawl:AbstractAllocator a yawl:Class ;
    rdfs:label "AbstractAllocator" ;
    yawl:inPackage yawl:org_yawlfoundation_yawl_resourcing_allocators ;
    yawl:filePath "vendors/yawl-v5.2/src/org/yawlfoundation/yawl/resourcing/allocators/AbstractAllocator.java"^^xsd:string ;
    rdfs:comment """The base class for all allocators. Create Date: 03/08/2007. Last Date: 09/11/2007 @author Michael Adams (BPM Group, QUT Australia) @version 2.0""" ;
    yawl:extends yawl:AbstractSelector ;
    yawl:modifiers "abstract" ;
    yawl:modifiers "public" ;
    yawl:hasMethod yawl:AbstractAllocator_toXML_1 ;
    yawl:hasMethod yawl:AbstractAllocator_getLoggedEvents_2 ;
    yawl:hasMethod yawl:AbstractAllocator_participantSetToMap_3 ;
    yawl:hasMethod yawl:AbstractAllocator_allocateOnStatus_4 ;
    yawl:hasMethod yawl:AbstractAllocator_getCostMap_5 ;
    yawl:hasMethod yawl:AbstractAllocator_getCost_6 ;
    yawl:hasMethod yawl:AbstractAllocator_getAvgDurations_7 ;
    yawl:hasMethod yawl:AbstractAllocator_getAvgDurations_8 ;
    yawl:hasMethod yawl:AbstractAllocator_pairEvents_9 ;
    yawl:hasMethod yawl:AbstractAllocator_getParticipantTimes_10 ;
    yawl:hasMethod yawl:AbstractAllocator_getAverage_11 ;
    yawl:hasMethod yawl:AbstractAllocator_performAllocation_12 ;
    .


# Method: AbstractAllocator.toXML
yawl:AbstractAllocator_toXML_1 a yawl:Method ;
    rdfs:label "toXML" ;
    yawl:returnType "String" ;
    yawl:signature "String toXML()" ;
    rdfs:comment """Generates the XML required for the specification file @return an XML'd String containing the member values of an instantiation of this class""" ;
    yawl:modifiers "public" ;
    .

# Method: AbstractAllocator.getLoggedEvents
yawl:AbstractAllocator_getLoggedEvents_2 a yawl:Method ;
    rdfs:label "getLoggedEvents" ;
    yawl:returnType "List" ;
    yawl:signature "List getLoggedEvents(WorkItemRecord wir, EventLogger event)" ;
    rdfs:comment """Gets a list of all resource log rows for a given specification + task + event combination. @param wir a workitem record which is an instance of the specification+task in question. @param event the type of event to get records for @return the matching list of ResourceEvent objects (each one a row of the log)""" ;
    yawl:modifiers "protected" ;
    .

# Method: AbstractAllocator.participantSetToMap
yawl:AbstractAllocator_participantSetToMap_3 a yawl:Method ;
    rdfs:label "participantSetToMap" ;
    yawl:returnType "Map" ;
    yawl:signature "Map participantSetToMap(Set pSet)" ;
    rdfs:comment """Converts a set of participants to a map @param pSet the set of participants @return a map of participant id to participant""" ;
    yawl:modifiers "protected" ;
    .

# Method: AbstractAllocator.allocateOnStatus
yawl:AbstractAllocator_allocateOnStatus_4 a yawl:Method ;
    rdfs:label "allocateOnStatus" ;
    yawl:returnType "Participant" ;
    yawl:signature "Participant allocateOnStatus(Set participants, WorkItemRecord wir, EventLogger status)" ;
    yawl:modifiers "public" ;
    .

# Method: AbstractAllocator.getCostMap
yawl:AbstractAllocator_getCostMap_5 a yawl:Method ;
    rdfs:label "getCostMap" ;
    yawl:returnType "Map" ;
    yawl:signature "Map getCostMap(Set pids, WorkItemRecord wir)" ;
    rdfs:comment """Gets from the cost service the cost per msec of each of the participants ids listed for the work item @param pids a set of participant ids @param wir the work item to allocate @return a map containing each participant id and its corresponding cost per msec to perform this work item""" ;
    yawl:modifiers "protected" ;
    .

# Method: AbstractAllocator.getCost
yawl:AbstractAllocator_getCost_6 a yawl:Method ;
    rdfs:label "getCost" ;
    yawl:returnType "double" ;
    yawl:signature "double getCost(XNode resNode, String duration)" ;
    yawl:modifiers "protected" ;
    .

# Method: AbstractAllocator.getAvgDurations
yawl:AbstractAllocator_getAvgDurations_7 a yawl:Method ;
    rdfs:label "getAvgDurations" ;
    yawl:returnType "Map" ;
    yawl:signature "Map getAvgDurations(EventLogger fromEvent, EventLogger toEvent, WorkItemRecord wir)" ;
    rdfs:comment """Gets the average duration (in msecs) between two events for each participant recorded against those events for a task @param fromEvent the earlier event (start of duration) @param toEvent the later event (end of duration) @param wir a work item to allocate @return a Map of participant ids and average durations""" ;
    yawl:modifiers "protected" ;
    .

# Method: AbstractAllocator.getAvgDurations
yawl:AbstractAllocator_getAvgDurations_8 a yawl:Method ;
    rdfs:label "getAvgDurations" ;
    yawl:returnType "Map" ;
    yawl:signature "Map getAvgDurations(Collection pairs)" ;
    yawl:modifiers "protected" ;
    .

# Method: AbstractAllocator.pairEvents
yawl:AbstractAllocator_pairEvents_9 a yawl:Method ;
    rdfs:label "pairEvents" ;
    yawl:returnType "Map" ;
    yawl:signature "Map pairEvents(List fromEvents, List toEvents)" ;
    rdfs:comment """Merges a list of starting events and a list of ending events for a task's instances into a map of event pairs @param fromEvents the list of earlier events (start of duration) @param toEvents the list of later events (end of duration) @return a map of work item ids and the corresponding event pair""" ;
    yawl:modifiers "private" ;
    .

# Method: AbstractAllocator.getParticipantTimes
yawl:AbstractAllocator_getParticipantTimes_10 a yawl:Method ;
    rdfs:label "getParticipantTimes" ;
    yawl:returnType "Map" ;
    yawl:signature "Map getParticipantTimes(Collection pairedEvents)" ;
    rdfs:comment """Gets a set of durations for each participant referenced by a map of event pairs @param pairedEvents a set of event pairs @return a map of participant ids, each to a set of durations (in msecs)""" ;
    yawl:modifiers "private" ;
    .

# Method: AbstractAllocator.getAverage
yawl:AbstractAllocator_getAverage_11 a yawl:Method ;
    rdfs:label "getAverage" ;
    yawl:returnType "long" ;
    yawl:signature "long getAverage(Set setOfLongs)" ;
    rdfs:comment """Gets the average of a set of long values @param setOfLongs the set @return the average of the set's values""" ;
    yawl:modifiers "private" ;
    .

# Method: AbstractAllocator.performAllocation
yawl:AbstractAllocator_performAllocation_12 a yawl:Method ;
    rdfs:label "performAllocation" ;
    yawl:returnType "Participant" ;
    yawl:signature "Participant performAllocation(Set resources, WorkItemRecord wir)" ;
    rdfs:comment """Performs an allocation using some strategy @param resources the distribution set of participants @param wir the work item to allocate @return the Participant chosen by the allocation strategy""" ;
    yawl:modifiers "abstract" ;
    yawl:modifiers "public" ;
    .





