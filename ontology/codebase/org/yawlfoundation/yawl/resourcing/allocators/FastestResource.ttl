# Class: org.yawlfoundation.yawl.resourcing.allocators.FastestResource
yawl:FastestResource a yawl:Class ;
    rdfs:label "FastestResource" ;
    yawl:inPackage yawl:org_yawlfoundation_yawl_resourcing_allocators ;
    yawl:filePath "vendors/yawl-v5.2/src/org/yawlfoundation/yawl/resourcing/allocators/FastestResource.java"^^xsd:string ;
    rdfs:comment """Performs allocation based on the least average cost to allocate a work item @author Michael Adams""" ;
    yawl:extends yawl:AbstractAllocator ;
    yawl:modifiers "public" ;
    yawl:hasMethod yawl:FastestResource_performAllocation_1 ;
    yawl:hasMethod yawl:FastestResource_getFastestAverage_2 ;
    yawl:hasMethod yawl:FastestResource_processEvents_3 ;
    .


# Method: FastestResource.performAllocation
yawl:FastestResource_performAllocation_1 a yawl:Method ;
    rdfs:label "performAllocation" ;
    yawl:returnType "Participant" ;
    yawl:signature "Participant performAllocation(Set participants, WorkItemRecord wir)" ;
    rdfs:comment """Selects a single participant from the list provided that has the lowest cost rate for the work item in question @param participants a distribution set of participants @param wir the work item to allocate @return a single participant, or null if participants is null or empty""" ;
    yawl:modifiers "public" ;
    .

# Method: FastestResource.getFastestAverage
yawl:FastestResource_getFastestAverage_2 a yawl:Method ;
    rdfs:label "getFastestAverage" ;
    yawl:returnType "String" ;
    yawl:signature "String getFastestAverage(Collection pSet, WorkItemRecord wir)" ;
    rdfs:comment """Gets the participant id in the map corresponding to the lowest cost amount @param pSet a map of participant ids and their associated costs @param wir the work item to allocate @return the participant id with the lowest cost""" ;
    yawl:modifiers "private" ;
    .

# Method: FastestResource.processEvents
yawl:FastestResource_processEvents_3 a yawl:Method ;
    rdfs:label "processEvents" ;
    yawl:returnType "void" ;
    yawl:signature "void processEvents(List events, Map pairs)" ;
    yawl:modifiers "private" ;
    .





