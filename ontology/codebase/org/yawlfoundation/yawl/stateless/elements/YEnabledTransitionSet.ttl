    yawl:hasMethod yawl:YEnabledTransitionSet_getAllTaskGroups_2 ;
    yawl:hasMethod yawl:YEnabledTransitionSet_isEmpty_3 ;
    yawl:hasMethod yawl:YEnabledTransitionSet_add_4 ;
    yawl:hasMethod yawl:YEnabledTransitionSet_getFlowsFromIDs_5 ;
    yawl:hasMethod yawl:YEnabledTransitionSet_isEnablingCondition_6 ;
    yawl:hasField yawl:YEnabledTransitionSet_transitions ;
    .


