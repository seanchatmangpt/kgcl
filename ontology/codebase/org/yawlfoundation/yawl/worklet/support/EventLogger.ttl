    yawl:filePath "vendors/yawl-v5.2/src/org/yawlfoundation/yawl/worklet/support/EventLogger.java"^^xsd:string ;
    rdfs:comment """An event log file implementation. @author Michael Adams v0.8, 04-09/2006""" ;
