yawl:YAtomicTask_cancelBusyWorkItem_5 a yawl:Method ;
    rdfs:label "cancelBusyWorkItem" ;
    yawl:returnType "boolean" ;
    yawl:signature "boolean cancelBusyWorkItem()" ;
    yawl:modifiers "private" ;
    .

# Method: YAtomicTask.cancelWorkItem
yawl:YAtomicTask_cancelWorkItem_6 a yawl:Method ;
    rdfs:label "cancelWorkItem" ;
    yawl:returnType "void" ;
    yawl:signature "void cancelWorkItem(YWorkItem workItem)" ;
    yawl:modifiers "private" ;
    .

# Method: YAtomicTask.t_rollBackToFired
yawl:YAtomicTask_t_rollBackToFired_7 a yawl:Method ;
    rdfs:label "t_rollBackToFired" ;
    yawl:returnType "boolean" ;
    yawl:signature "boolean t_rollBackToFired(YIdentifier caseID)" ;
    rdfs:comment """Rolls back a task's inner state from 'executing' to 'entered'. <p/> The roll back will only occur if the task's inner 'executing' marking contains the identifier specified. @param caseID the case identifier of this atomic task. @return true if the rollback occurs, false if the rollback request could not be actioned because the task's inner 'executing' marking does not contain  the identifier specified.""" ;
    yawl:modifiers "public" ;
    .

# Method: YAtomicTask.clone
yawl:YAtomicTask_clone_8 a yawl:Method ;
    rdfs:label "clone" ;
    yawl:returnType "Object" ;
    yawl:signature "Object clone()" ;
    rdfs:comment """Clones this atomic task. @return a (Object) clone of the task. @throws CloneNotSupportedException if there's a problem cloning the task.""" ;
    yawl:modifiers "public" ;
    .

# Method: YAtomicTask.verify
yawl:YAtomicTask_verify_9 a yawl:Method ;
    rdfs:label "verify" ;
    yawl:returnType "void" ;
    yawl:signature "void verify(YVerificationHandler handler)" ;
    rdfs:comment """Verifies this atomic task definition against YAWL semantics. @return a List of error and/or warning messages. An empty list is returned if the atomic task verifies successfully.""" ;
    yawl:modifiers "public" ;
    .





